{
  "name": "scheduler-api",
  "version": "1.0.0",
  "description": "Scheduler API built on node, express, and objection",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "lint-all": "eslint ./**/*.js",
    "start": "node ./server.js",
    "devstart": "cross-env NODE_ENV=development nodemon ./server.js",
    "sslstart": "yarn local-ssl-proxy --source 443 --target 3333",
    "prodstart": "cross-env NODE_ENV=production yarn start",
    "migrate": "npx knex migrate:latest",
    "seed": "npx knex seed:run",
    "sqlite-migrate": "cross-env NODE_ENV=sqlite3 yarn knex migrate:latest",
    "sqlite-seed": "cross-env NODE_ENV=sqlite3  yarn knex seed:run",
    "sqlite-start": "cross-env NODE_ENV=sqlite3  yarn start"
  },
  "repository": "https://github.com/john-lednicky/scheduler-api-objection-node",
  "keywords": [
    "node",
    "javascript",
    "express",
    "objection",
    "jest",
    "swagger",
    "openapi"
  ],
  "author": "John Lednicky",
  "license": "MIT",
  "dependencies": {
    "date-fns": "^2.22.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.3",
    "express-winston": "^4.1.0",
    "helmet": "^4.6.0",
    "http-errors": "^1.8.0",
    "knex": "^0.95.4",
    "mysql2": "^2.2.5",
    "objection": "^2.2.15",
    "serve-favicon": "^2.5.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "jest": "^27.0.6",
    "local-ssl-proxy": "^1.3.0",
    "nodemon": "^2.0.9",
    "sqlite3": "^5.0.2"
  }
}
