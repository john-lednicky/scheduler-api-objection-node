{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "EventType",
    "description": "An activity that can be scheduled as an event on the calendar.",
    "type": "object",
    "required": [
        "eventTypeId",
        "beginDttm",
        "endDttm",
        "updateUser",
        "updateDttm"
    ],
    "properties": {
        "id": {
            "type": "integer"
        },
        "eventTypeId": {
            "type": "integer"
        },
        "beginDttm": {
            "type": "string",
            "pattern": "^\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}(.\\d{2})?$"
        },
        "endDttm": {
            "type": "string",
            "pattern": "^\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}(.\\d{2})?$"
        },
        "peopleNeeded": {
            "type": "integer",
            "minimum": 1,
            "maximum": 25
        },
        "comment": {
            "type": "string",
            "minLength": 0,
            "maxLength": 500,
            "pattern": "^[a-zA-Z0-9 .,:;\\-?!&/\\\\']*$"
        },
        "updateUser": {
            "type": "string",
            "minLength": 1,
            "maxLength": 45,
            "pattern": "^[a-zA-Z0-9 .-@]+$"
        },
        "updateDttm": {
            "type": "string",
            "pattern": "^\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}(.\\d{2})?$"
        }
    }
}