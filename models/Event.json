{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "EventType",
    "description": "An activity that can be scheduled as an event on the calendar.",
    "type": "object",
    "required": [
        "eventTypeId",
        "beginDttm",
        "endDttm",
        "updateUser",
        "updateDttm"
    ],
    "properties": {
        "id": {
            "type": "integer",
            "example": 1320
        },
        "eventTypeId": {
            "type": "integer",
            "example": 3
        },
        "beginDttm": {
            "type": "string",
            "pattern": "^\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}(.\\d{2})?$",
            "example": "2021-03-08 11:00:00"
        },
        "endDttm": {
            "type": "string",
            "pattern": "^\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}(.\\d{2})?$",
            "example": "2021-03-08 11:00:00"
        },
        "peopleNeeded": {
            "type": "integer",
            "minimum": 1,
            "maximum": 25,
            "example": 3
        },
        "comment": {
            "type": "string",
            "minLength": 0,
            "maxLength": 500,
            "pattern": "^[a-zA-Z0-9 .,:;\\-?!&/\\\\']*$",
            "example": "On this date, we will be deep-cleaning the walk-in."
        },
        "updateUser": {
            "type": "string",
            "minLength": 1,
            "maxLength": 45,
            "pattern": "^[a-zA-Z0-9 .-@]+$",
            "example": "john.d.lednicky"
        },
        "updateDttm": {
            "type": "string",
            "pattern": "^\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}(.\\d{2})?$",
            "example": "2021-03-08 11:00:00"
        }
    }
}